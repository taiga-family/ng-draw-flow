{
    "name": "ng-draw-flow",
    "version": "0.0.6",
    "description": "A customizable Angular component for building node-based editors and interactive diagrams",
    "keywords": [
        "draw flow",
        "ng",
        "angular",
        "graph",
        "editor",
        "flow",
        "draw",
        "diagram",
        "chart"
    ],
    "homepage": "https://github.com/taiga-family/ng-draw-flow",
    "bugs": "https://github.com/taiga-family/ng-draw-flow/issues",
    "repository": "https://github.com/taiga-family/ng-draw-flow",
    "license": "Apache-2.0",
    "author": {
        "name": "Airat Khairullin",
        "email": "a1rat91@yandex.ru",
        "url": "https://t.me/a1rat91"
    },
    "scripts": {
        "prepare": "husky",
        "start": "nx serve demo",
        "test": "nx test",
        "lint": "eslint .",
        "prettier": "prettier '**/*.{svg,yml,js,ts,html,md,less,json}'",
        "stylelint": "stylelint '**/*.{less,css}' --config package.json",
        "bump": "npx syncer && npx nx run-many -t publish"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.less": [
            "stylelint --fix"
        ],
        "*.{ts,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{svg,yml,md,less,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "eslintConfig": {
        "extends": [
            "plugin:@taiga-ui/experience/all"
        ],
        "rules": {
            "no-restricted-syntax": "off",
            "@typescript-eslint/consistent-type-assertions": "off",
            "@typescript-eslint/member-ordering": "off",
            "rxjs/no-implicit-any-catch": "off",
            "sonarjs/prefer-nullish-coalescing": "off",
            "sonarjs/redundant-type-aliases": "off",
            "@taiga-ui/experience/strict-tui-doc-example": "off"
        },
        "root": true
    },
    "stylelint": {
        "extends": [
            "@taiga-ui/stylelint-config"
        ]
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "16.0.0",
        "@angular-devkit/build-angular": "16.2.12",
        "@angular-devkit/core": "16.2.12",
        "@angular-devkit/schematics": "16.2.12",
        "@angular-devkit/schematics-cli": "0.901.13",
        "@angular/animations": "16.2.12",
        "@angular/cdk": "16.2.12",
        "@angular/cli": "16.2.12",
        "@angular/common": "16.2.12",
        "@angular/compiler": "16.2.12",
        "@angular/compiler-cli": "16.2.12",
        "@angular/core": "16.2.12",
        "@angular/forms": "16.2.12",
        "@angular/platform-browser": "16.2.12",
        "@angular/platform-browser-dynamic": "16.2.12",
        "@angular/platform-server": "16.2.12",
        "@angular/router": "16.2.12",
        "@ng-web-apis/common": "3.0.6",
        "@ng-web-apis/resize-observer": "3.0.6",
        "@ng-web-apis/universal": "3.0.7",
        "@nguniversal/builders": "16.2.0",
        "@nguniversal/express-engine": "16.2.0",
        "@nx/angular": "18.2.1",
        "@nx/jest": "18.2.1",
        "@nx/js": "18.2.1",
        "@nx/node": "18.2.1",
        "@nx/workspace": "18.2.1",
        "@schematics/angular": "16.0.0",
        "@taiga-ui/addon-doc": "3.94.0",
        "@taiga-ui/addon-mobile": "3.94.0",
        "@taiga-ui/auto-changelog-config": "0.204.0",
        "@taiga-ui/browserslist-config": "0.6.0",
        "@taiga-ui/cdk": "3.94.0",
        "@taiga-ui/commitlint-config": "0.7.5",
        "@taiga-ui/core": "3.94.0",
        "@taiga-ui/eslint-plugin-experience": "0.204.0",
        "@taiga-ui/icons": "3.94.0",
        "@taiga-ui/kit": "3.94.0",
        "@taiga-ui/prettier-config": "0.204.0",
        "@taiga-ui/release-it-config": "0.204.0",
        "@taiga-ui/stylelint-config": "0.204.0",
        "@taiga-ui/styles": "3.94.0",
        "@taiga-ui/syncer": "0.204.0",
        "@taiga-ui/tsconfig": "0.128.0",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.13",
        "core-js": "2.6.12",
        "express": "4.21.0",
        "http-server": "14.1.1",
        "husky": "9.1.6",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "14.0.3",
        "lint-staged": "12.3.7",
        "ng-packagr": "16.0.0",
        "ngx-markdown": "16.0.0",
        "nx": "18.2.1",
        "rxjs": "7.8.0",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.2",
        "tslib": "2.7.0",
        "tsutils": "3.21.0",
        "typescript": "4.9.5",
        "webpack": "5.94.0",
        "webpack-merge": "5.10.0"
    },
    "syncer": {
        "includePaths": [
            "./projects",
            "./package-lock.json"
        ],
        "matchPackageNames": [
            "@ng-draw-flow/*",
            "ng-draw-flow"
        ],
        "ignorePackageNames": []
    }
}
