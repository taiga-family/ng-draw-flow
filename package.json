{
    "name": "ng-draw-flow",
    "version": "0.13.0",
    "description": "A customizable Angular component for building node-based editors and interactive diagrams",
    "keywords": [
        "draw flow",
        "ng",
        "angular",
        "graph",
        "editor",
        "flow",
        "draw",
        "diagram",
        "chart"
    ],
    "homepage": "https://github.com/taiga-family/ng-draw-flow",
    "bugs": "https://github.com/taiga-family/ng-draw-flow/issues",
    "repository": "https://github.com/taiga-family/ng-draw-flow",
    "license": "Apache-2.0",
    "author": {
        "name": "Airat Khairullin",
        "email": "a1rat91@yandex.ru",
        "url": "https://t.me/a1rat91"
    },
    "scripts": {
        "prepare": "husky",
        "start": "nx serve demo",
        "test": "nx test ng-draw-flow",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --write !package-lock.json . --ignore-path .gitignore",
        "stylelint": "stylelint '**/*.{less,css}' --config package.json",
        "stylelint:fix": "stylelint '**/*.{less,css}' --config package.json --fix",
        "release": "npx syncer && npx nx run-many -t publish",
        "build": "npx nx run-many --target=build --all --configuration=production"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.less": [
            "stylelint --fix"
        ],
        "*.{ts,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{svg,yml,md,less,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "eslintConfig": {
        "extends": [
            "plugin:@taiga-ui/experience/all"
        ],
        "rules": {
            "no-restricted-syntax": "off",
            "@typescript-eslint/consistent-type-assertions": "off",
            "@typescript-eslint/member-ordering": "off",
            "rxjs/no-implicit-any-catch": "off",
            "sonarjs/no-gratuitous-expressions": "off",
            "sonarjs/prefer-nullish-coalescing": "off",
            "sonarjs/redundant-type-aliases": "off",
            "@taiga-ui/experience/strict-tui-doc-example": "off"
        },
        "root": true
    },
    "stylelint": {
        "extends": [
            "@taiga-ui/stylelint-config"
        ]
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "16.0.1",
        "@angular-devkit/build-angular": "16.2.16",
        "@angular-devkit/core": "16.2.16",
        "@angular-devkit/schematics": "16.2.16",
        "@angular-devkit/schematics-cli": "0.1102.19",
        "@angular/animations": "16.2.12",
        "@angular/cdk": "16.2.14",
        "@angular/cli": "16.2.16",
        "@angular/common": "16.2.12",
        "@angular/compiler": "16.2.12",
        "@angular/compiler-cli": "16.2.12",
        "@angular/core": "16.2.12",
        "@angular/forms": "16.2.12",
        "@angular/platform-browser": "16.2.12",
        "@angular/platform-browser-dynamic": "16.2.12",
        "@angular/platform-server": "16.2.12",
        "@angular/router": "16.2.12",
        "@ng-web-apis/common": "4.12.0",
        "@ng-web-apis/resize-observer": "4.12.0",
        "@ng-web-apis/universal": "4.12.0",
        "@nguniversal/builders": "16.2.0",
        "@nguniversal/express-engine": "16.2.0",
        "@nx/angular": "18.3.5",
        "@nx/jest": "18.3.5",
        "@nx/js": "18.3.5",
        "@nx/node": "18.3.5",
        "@nx/workspace": "18.3.5",
        "@schematics/angular": "16.2.16",
        "@taiga-ui/addon-doc": "4.48.0",
        "@taiga-ui/addon-mobile": "4.48.0",
        "@taiga-ui/auto-changelog-config": "0.308.0",
        "@taiga-ui/browserslist-config": "0.308.0",
        "@taiga-ui/cdk": "4.48.0",
        "@taiga-ui/commitlint-config": "0.308.0",
        "@taiga-ui/core": "4.48.0",
        "@taiga-ui/eslint-plugin-experience": "0.249.0",
        "@taiga-ui/event-plugins": "4.6.0",
        "@taiga-ui/icons": "4.48.0",
        "@taiga-ui/kit": "4.48.0",
        "@taiga-ui/legacy": "4.48.0",
        "@taiga-ui/prettier-config": "0.308.0",
        "@taiga-ui/release-it-config": "0.308.0",
        "@taiga-ui/stylelint-config": "0.308.0",
        "@taiga-ui/styles": "4.48.0",
        "@taiga-ui/syncer": "0.308.0",
        "@taiga-ui/tsconfig": "0.308.0",
        "@types/express": "5.0.3",
        "@types/jest": "29.5.14",
        "core-js": "3.45.0",
        "express": "4.21.2",
        "http-server": "14.1.1",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "14.6.1",
        "lint-staged": "15.5.2",
        "ng-mocks": "14.13.5",
        "ng-packagr": "16.2.3",
        "ngx-highlightjs": "10.0.0",
        "ngx-markdown": "16.0.0",
        "nx": "18.3.5",
        "rxjs": "7.8.2",
        "ts-jest": "29.4.1",
        "ts-node": "10.9.2",
        "tslib": "2.8.1",
        "tsutils": "3.21.0",
        "typescript": "5.0.4",
        "webpack": "5.101.0",
        "webpack-merge": "6.0.1"
    },
    "syncer": {
        "includePaths": [
            "./projects",
            "./package-lock.json"
        ],
        "matchPackageNames": [
            "@ng-draw-flow/*",
            "ng-draw-flow"
        ],
        "ignorePackageNames": []
    }
}
